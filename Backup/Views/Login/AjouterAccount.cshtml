
@model RoadBetterTogether.UserAcompteSet

<fieldset>
    <legend>Enrégistrer vos informations d'authentification:</legend>
    <div class="editor-label">
        @Html.LabelFor(model => model.login)
    </div>
    <div class="editor-field">
        @Html.EditorFor(model => model.login, new Dictionary<string, object>{
                     {"id", "login"}})
        @Html.ValidationMessageFor(model => model.login)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.password)
    </div>
    <div class="editor-field">
        @Html.PasswordFor(model => model.password, new Dictionary<string, object>{
                     {"id", "password1"}})
        @Html.ValidationMessageFor(model => model.password)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.password)
    </div>
    <div class="editor-field">
        @Html.PasswordFor(model => model.password, new Dictionary<string, object>{
                     {"id", "password2"} })
        @Html.ValidationMessageFor(model => model.password)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.username)
    </div>
    <div class="editor-field">
        @Html.EditorFor(model => model.username, new Dictionary<string, object>{
                     {"id", "name"}})
        @Html.ValidationMessageFor(model => model.username)
    </div>

</fieldset>


